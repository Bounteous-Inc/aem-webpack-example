{
  "name": "webpack-core",
  "version": "1.0.0",
  "private": true,
  "description": "Core front end setup of a project",
  "scripts": {
    "build": "webpack --config internals/webpack.config.js",
    "build:debug": "BABEL_DISABLE_CACHE=1 npm run build",
    "build:watch": "npm run build -- --watch",
    "deploy": "NODE_ENV='production' npm run build -- -p",
    "postinstall": "npm rebuild node-sass",
    "test": "NODE_ENV=test jest --config ./internals/jest.config.js",
    "test:debug": "BABEL_DISABLE_CACHE=1 npm run test -- --no-cache",
    "test:watch": "npm run test -- --watch",
    "start": "ttab -w exec npm run test:watch && npm run build:watch"
  },
  "author": "Infield Digital",
  "license": "GPL-3",
  "devDependencies": {
    "babel-core": "^6.18.0",
    "babel-loader": "^7.1.1",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.24.1",
    "babel-preset-jest": "^20.0.3",
    "babel-preset-latest": "^6.24.1",
    "css-loader": "^0.26.1",
    "eslint": "^3.15.0",
    "eslint-loader": "^1.9.0",
    "extract-text-webpack-plugin": "^2.1.2",
    "jest": "^19.0.2",
    "jest-cli": "^19.0.2",
    "node-sass": "^4.1.1",
    "postcss-loader": "^1.2.1",
    "sass-loader": "^6.0.2",
    "stylelint": "^7.12.0",
    "ttab": "^0.6.0",
    "webpack": "^3.1.0"
  },
  "dependencies": {
    "autoprefixer": "^7.1.2",
    "babel-polyfill": "^6.23.0"
  }
}
