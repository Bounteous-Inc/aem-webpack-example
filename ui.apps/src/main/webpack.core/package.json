{
  "name": "webpack-core",
  "version": "2.0.0",
  "private": true,
  "description": "Core front end setup of a project",
  "scripts": {
    "build": "webpack --config internals/webpack.config.js",
    "build:debug": "BABEL_DISABLE_CACHE=1 npm run build",
    "build:watch": "npm run build -- --watch",
    "deploy": "NODE_ENV=production npm run build -- -p",
    "postinstall": "npm rebuild node-sass",
    "test": "NODE_ENV=test jest --config internals/jest.config.js",
    "test:debug": "BABEL_DISABLE_CACHE=1 npm run test -- --no-cache",
    "test:watch": "npm run test -- --watch",
    "start": "FORCE_COLOR=1 CI=true npm-run-all --parallel build:watch test:watch"
  },
  "author": "Infield Digital",
  "license": "GPL-3",
  "devDependencies": {
    "autoprefixer": "^7.1.2",
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.1",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.24.1",
    "babel-preset-env": "^1.6.0",
    "babel-preset-jest": "^20.0.3",
    "css-loader": "^0.28.7",
    "eslint": "^4.11.0",
    "eslint-loader": "^1.9.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "jest": "^21.2.1",
    "jest-cli": "^21.2.1",
    "merge": "^1.2.0",
    "node-sass": "^4.7.2",
    "npm-run-all": "^4.1.2",
    "postcss-loader": "^2.0.8",
    "sass-loader": "^6.0.6",
    "stylelint": "^7.13.0",
    "webpack": "^3.8.1",
    "webpack-merge": "^4.1.1"
  },
  "dependencies": {
    "babel-polyfill": "^6.23.0"
  }
}
