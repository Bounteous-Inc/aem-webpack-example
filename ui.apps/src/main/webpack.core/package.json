{
  "name": "aem-webpack-example",
  "version": "2.5.1",
  "private": true,
  "description": "Core front end setup of a project",
  "scripts": {
    "aem:watch": "aem-front -w ../content/jcr_root -e **/webpack.module/**",
    "build": "webpack --config internals/webpack.config.js",
    "build:debug": "BABEL_DISABLE_CACHE=1 npm run build",
    "build:watch": "npm run build -- --watch",
    "production": "cross-env NODE_ENV=production \"${npm_node_execpath}\" \"${npm_execpath}\" run build -- -p",
    "postinstall": "cross-env \"${npm_node_execpath}\" \"${npm_execpath}\" rebuild node-sass",
    "start": "FORCE_COLOR=1 CI=true concurrently --prefix \"[ {name} ]\"  --names \"AEM Front, Webpack \" --prefix-colors \"white.bgCyan.bold,white.bgGreen.bold\" \"npm run aem:watch\" \"npm run build:watch\" --kill-others",
    "test": "cross-env NODE_ENV=test jest --config internals/jest.config.js",
    "test:debug": "BABEL_DISABLE_CACHE=1 npm run test -- --no-cache",
    "test:watch": "npm run test -- --watch"
  },
  "author": "Infield Digital",
  "license": "GPL-3",
  "devDependencies": {
    "aem-front": "^0.2.0",
    "autoprefixer": "^8.6.5",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "babel-preset-env": "^1.7.0",
    "babel-preset-jest": "^22.4.4",
    "concurrently": "^3.6.0",
    "cross-env": "^5.2.0",
    "css-loader": "^0.28.11",
    "eslint": "^4.19.1",
    "eslint-loader": "^2.0.0",
    "jest": "^22.4.4",
    "jest-cli": "^22.4.3",
    "merge": "^1.2.0",
    "mini-css-extract-plugin": "^0.4.1",
    "node-sass": "^4.9.2",
    "postcss-loader": "^2.1.6",
    "postcss-reporter": "^5.0.0",
    "sass-loader": "^7.0.3",
    "stylelint": "^9.3.0",
    "webpack": "^4.16.1",
    "webpack-command": "^0.4.1"
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0"
  }
}
